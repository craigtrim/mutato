@prefix : <http://test.ai/music#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@base <http://test.ai/music#> .

<http://test.ai/music> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://test.ai/music#implies
:implies rdf:type owl:AnnotationProperty .

###  http://test.ai/music#inflection
:inflection rdf:type owl:AnnotationProperty ;
            rdfs:label "inflection"@en ;
            rdfs:subPropertyOf skos:altLabel .

###  http://test.ai/music#requires
:requires rdf:type owl:AnnotationProperty .

###  http://test.ai/music#similarTo
:similarTo rdf:type owl:AnnotationProperty .

###  http://test.ai/music#uses
:uses rdf:type owl:AnnotationProperty .

###  http://www.w3.org/2004/02/skos/core#altLabel
skos:altLabel rdf:type owl:AnnotationProperty .

###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty .

#################################################################
#    Classes
#################################################################

###  http://test.ai/music#Music_Topic
:Music_Topic rdf:type owl:Class ;
             rdfs:label "Music Topic" ;
             skos:altLabel "music" .

###  http://test.ai/music#Instrument
:Instrument rdf:type owl:Class ;
            rdfs:subClassOf :Music_Topic ;
            rdfs:label "Instrument" ;
            skos:altLabel "musical instrument" .

###  http://test.ai/music#Genre
:Genre rdf:type owl:Class ;
       rdfs:subClassOf :Music_Topic ;
       rdfs:label "Genre" ;
       skos:altLabel "music genre" .

###  http://test.ai/music#Concept
:Concept rdf:type owl:Class ;
         rdfs:subClassOf :Music_Topic ;
         rdfs:label "Concept" .

### ---- Instrument Families ----

###  http://test.ai/music#String_Instrument
:String_Instrument rdf:type owl:Class ;
                   rdfs:subClassOf :Instrument ;
                   rdfs:label "String Instrument" .

###  http://test.ai/music#Wind_Instrument
:Wind_Instrument rdf:type owl:Class ;
