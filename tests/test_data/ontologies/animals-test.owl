@prefix : <http://test.ai/animals#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@base <http://test.ai/animals#> .

<http://test.ai/animals> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://test.ai/animals#implies
:implies rdf:type owl:AnnotationProperty .

###  http://test.ai/animals#inflection
:inflection rdf:type owl:AnnotationProperty ;
            rdfs:label "inflection"@en ;
            rdfs:subPropertyOf skos:altLabel .

###  http://test.ai/animals#requires
:requires rdf:type owl:AnnotationProperty .

###  http://test.ai/animals#similarTo
:similarTo rdf:type owl:AnnotationProperty .

###  http://test.ai/animals#uses
:uses rdf:type owl:AnnotationProperty .

###  http://test.ai/animals#locatedIn
:locatedIn rdf:type owl:AnnotationProperty .

###  http://www.w3.org/2004/02/skos/core#altLabel
skos:altLabel rdf:type owl:AnnotationProperty .

###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty .

#################################################################
#    Classes
#################################################################

###  http://test.ai/animals#Animal
:Animal rdf:type owl:Class ;
        rdfs:label "Animal" .

###  http://test.ai/animals#Mammal
:Mammal rdf:type owl:Class ;
        rdfs:subClassOf :Animal ;
        rdfs:label "Mammal" .

###  http://test.ai/animals#Bird
:Bird rdf:type owl:Class ;
      rdfs:subClassOf :Animal ;
      rdfs:label "Bird" .

###  http://test.ai/animals#Reptile
:Reptile rdf:type owl:Class ;
         rdfs:subClassOf :Animal ;
         rdfs:label "Reptile" .

###  http://test.ai/animals#Fish
:Fish rdf:type owl:Class ;
      rdfs:subClassOf :Animal ;
      rdfs:label "Fish" .

###  http://test.ai/animals#Insect
:Insect rdf:type owl:Class ;
        rdfs:subClassOf :Animal ;
        rdfs:label "Insect" .

### ---- Mammals ----

###  http://test.ai/animals#Dog
:Dog rdf:type owl:Class ;
     rdfs:subClassOf :Mammal ;
     rdfs:label "Dog" ;
     skos:altLabel "canine" ;
     :inflection "dogs" .

