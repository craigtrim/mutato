             :locatedIn :United_States .

###  http://test.ai/geography#Chicago
:Chicago rdf:type owl:Class ;
         rdfs:subClassOf :City ;
         rdfs:label "Chicago" ;
         :locatedIn :United_States .

###  http://test.ai/geography#London
:London rdf:type owl:Class ;
        rdfs:subClassOf :City ;
        rdfs:label "London" ;
        :locatedIn :United_Kingdom .

###  http://test.ai/geography#Paris
:Paris rdf:type owl:Class ;
       rdfs:subClassOf :City ;
       rdfs:label "Paris" ;
       :locatedIn :France .

###  http://test.ai/geography#Tokyo
:Tokyo rdf:type owl:Class ;
       rdfs:subClassOf :City ;
       rdfs:label "Tokyo" ;
       :locatedIn :Japan .

###  http://test.ai/geography#Beijing
:Beijing rdf:type owl:Class ;
         rdfs:subClassOf :City ;
         rdfs:label "Beijing" ;
         skos:altLabel "peking" ;
         :locatedIn :China .

###  http://test.ai/geography#Sydney
:Sydney rdf:type owl:Class ;
        rdfs:subClassOf :City ;
        rdfs:label "Sydney" .

###  http://test.ai/geography#Mumbai
:Mumbai rdf:type owl:Class ;
        rdfs:subClassOf :City ;
        rdfs:label "Mumbai" ;
